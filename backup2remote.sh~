#!/bin/sh
RSYNC=/usr/bin/rsync
SSH=/usr/bin/ssh
#KEY=/home/localuser/.ssh /id_rsa
RUSER=developer
RHOST=211.166.10.154
RPATH=/home/developer/ubuntu
LPATH=/home/broader/develop/R@K
