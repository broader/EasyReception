			window.addEvent('domready', function(){
             new FormCheck('formular',{       
               //submit: false,
               display:{
                  //showErrors : 1,
                  //titlesInsteadNames : 0,
                  errorsLocation : 2,
                  //indicateErrors : 1,
                  //indicateErrorsInit : 0,
                  //keepFocusOnError : 0, 
                  //scrollToFirst : true
               }
             });
});
		</script>



